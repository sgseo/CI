/*公共部分*/
.icon{display:inline-block;vertical-align:middle;text-align:center;background:url("../images/icon.png") 0 0 no-repeat;}
.icon.icon-phone{width:20px;height:25px;}
.icon.icon-sina{width:120px;height:42px;background-position:0 -25px;}
.icon.icon-news-sign{width:13px;height:12px;background-position:-20px 0px;}
.icon.icon-qzone{width:115px;height:36px;background-position:0 -67px;}
.icon.icon-wx{width:60px;height:45px;background-position:0 -104px;}
.icon.icon-kf{width:41px;height:42px;background-position:0 -153px;}
.icon.icon-hot{width:37px;height:24px;background-position:0 -202px;}
.icon.icon-up{width:15px;height:10px;background-position:-35px 0px;}
.icon.icon-down{width:15px;height:10px;background-position:-51px 0px;}
.mr10{margin-right:10px;}
.mr20{margin-right:20px;}
.mt20{margin-top:20px;}
.mr30{margin-right:30px;}
.ml10{margin-left:10px;}
.col-f39800{color:#f39800;}
.col-2fa8e1{color:#2fa8e1;}
a.col-2fa8e1:hover{color:#00b7ee;}
.triangle{width:0;height:0;display:inline-block;vertical-align:middle;border-left:5px solid transparent;border-top:5px solid #666666;border-right:5px solid transparent;}
.hr-style{height:1px;border:none;background:#c4c4c4;}
.room-left{width:170px;}
.room-right{width:790px;}
.room-modify{background:#fff;border:1px solid #e0e0e0;min-height:490px;padding:20px 30px 30px 30px;}
.room-info h1{font-size:24px;color:#f39700;line-height:40px;margin-bottom:20px;}
.room-info h4{font-size:24px;color:#f39700;line-height:40px;margin-bottom:20px;}
.room-info h5{font-size:18px;color:#ffffff;line-height:40px;margin:0px 0 20px -30px;height:40px;background:#f8b551;float:left;padding:0 20px;}
.room-info h6{font-size:18px;color:#f39700;line-height:40px;margin:0 0 0px 0px;}
.room-info .phc_abt{font-size:18px;color:#f39700;line-height:40px;margin:0 0 0px 0px;}
.room-info .border-demo{border:1px solid #e0e0e0;padding:30px 20px 10px 20px;margin-bottom:20px;}
.room-info p{font-size:14px;color:#2e2e2e;line-height:26px;margin-bottom:20px;}
.room-info p.text-indent{text-indent:2em;}
.room-info p.text-big,.room-info p.text-big a{font-size:14px;}
.room-info p strong{font-size:16px;font-weight:normal;}
.room-img-center{text-align:center;padding:20px 0 0 0;}
/*头部*/
.header-top{height:30px;line-height:30px;background:#666666;color:#ffffff}
.member-control a{color:#fff;}
.member-control a.col-f39800{color:#f39800}
.member-control a:hover,.member-control a.col-f39800:hover{color:#2fa8e1;}
.header{height:90px;background:#ffffff;border-bottom:1px solid #bababa;}
.logo{text-indent:-99999px;width:350px;height:65px;margin-top:12.5px;}
.nav-top-list{height:30px;margin-top:30px;}
.nav-top-list li{float:left;padding-left:35px;}
.nav-top-list li a{font-size:18px;color:#2e2e2e;line-height:30px;}
.nav-top-list li a:hover{color:#2fa8e1;}
.nav-top-list li a:hover .triangle{border-top-color:#2fa8e1;}
.nav-top-list li.hasmore{position:relative;}
/*边侧导航*/
.nav-aside-list{border:1px solid #e0e0e0;background:#ffffff;}
.nav-aside-list li{height:50px;border-bottom:1px solid #e0e0e0;}
.nav-aside-list li a{font-size:14px;color:#2e2e2e;line-height:50px;padding-left:25px;display:block;}
.nav-aside-list li a.cur,.nav-aside-list li a:hover{color:#ffffff;background:#f39700;}
.nav-aside-list li a:hover{background:#f39700;}
