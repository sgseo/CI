/*翻页*/
.page-list{margin:20px 0;}
.page-list li{float:left;height:27px;line-height:27px;}
.page-list.fl-left li{margin-left:5px;}
.page-list.fl-right li{margin-right:5px;}
.page-list li a{border:1px solid #e0e0e0;line-height:27px;color:#1b1b1b;display:block;padding:0 10px;}
.page-list li a.no-page,.page-list li a.no-page:hover{color:#a8a9ad;cursor:text;background:#ffffff;}
.page-list li a:hover,.page-list li a.cur{background:#00b7ee;color:#ffffff;}
.page-list li a:hover{background:#92dcfe;}
/*导航下拉菜单*/
#lr_systembox{ width:980px; height:50px; background-color:#CCC; position:relative; margin:0 auto; z-index:100000;}
.lr_systembtn{ width:201px; height:35px; line-height:33px;position:absolute; top:0px; right:0px; z-index:100004;}
.lr_systembtn .lr_abtn{ width:100%; height:35px; display:block; font-size:15px; font-weight:bold;color:#666666;}
.lr_systembtn .lr_abtn:hover{color:#333333;background-position:left bottom;}
.lr_systembtn .lr_abtn span{ padding-left:28px;}
.lr_menu{ width:184px; padding:8px 6px 8px 6px; background-color:#ffffff; border:#ACACAC solid 1px;filter:alpha(opacity=90);opacity: 0.90;
    position:absolute; top:60px; right:0px;  z-index:100005;display:none;}
.lr_menu dl{width:100%; display:block; overflow:hidden;}
.lr_menu a{ width:100%;display:block; color:#666666;border-bottom:#dddddd dashed 1px;height:30px; line-height:30px; font-size:14px;}
.lr_menu a:hover{background-color:#E2E2E2; color:#333333; text-decoration:none;}
.lr_menu dt a{text-indent:14px;font-size:16px;}
.lr_menu dd a{text-indent:24px;font-size:14px;color:#666666;}
/*表格*/
.table{width:100%;margin-bottom:20px;}
.table,.table tr,.table td{border-collapse:collapse;border:1px solid #999999;font-size:14px;}
.table td{line-height:30px;height:30px;text-align:center;}
.table.table-align-left td{text-align:left;text-indent:20px;}


/*安全保障*/
.me-aqbz{ width:961px; border:1px solid #e5e5e5; display:block;height:auto; overflow:hidden; padding:0 20px; background:#fff; margin:20px auto;}
.title-aqbz{ font-size:20px; font-weight:bold; color:#f35a00; line-height:50px; padding-top:15px; border-bottom:1px solid #e5e5e5;}
.table-aqbz{ width:960px; color:#666; line-height:25px; font-size:14px; padding-top:30px; float:left; padding-bottom:50px;}
.table-aqbz .pleft{ padding:0 80px; width:360px; float:left;vertical-align:center;}
.table-aqbz .pright{padding:0 80px 0 00px; width:300px; float:left;vertical-align:center;} 
.table-aqbz .prightss{padding:40px 80px 0 00px; width:300px; float:left;vertical-align:center;}
.table-aqbz span{ display:block; font-size:30px; line-height:58px;color:#222; padding-bottom:10px;}
.table-aqbz p{ padding-bottom:20px;color:#666; }