		var oDiv = $(this).find('.hp_cont');
		oBtn.click(function(){
			$('.hp_cont').slideUp(200);
			$('.help_box1').find('h5').removeClass('down');
			if(oDiv.is(":visible")){				
				oDiv.slideUp(200)
			} else {
				oBtn.addClass("down")
				oDiv.slideDown(300)
			}			
		});
	});
	
});
</SCRIPT>
