        <!-- 我的个人信息开始 -->

<table class="table table-bordered table-hover definewidth m10" >
    <thead>
    <tr>
     
        <th>借款标号</th>
        <th>借款标题</th>
        <th>客户姓名</th>
        <th>身份证号码</th>
        <th>联系电话</th>
        <th>贷款金额</th>
        <th>贷款期限</th>
        <th>贷款利率（%）</th>
        <th>借款类型</th>
        <th>状态</th>
        <th>操作</th>
        <!-- 客户姓名，身份证号码，联系电话，贷款金额，期限，利率， -->
    </tr>
    </thead>
    <volist name='list' id='vo'>
         <tr>
            <td>{$vo.id}</td>
            <td><a href='__APP__/Home/Node/showlog?id={$vo.id}'>{$vo.title}</a></td>
            <td>{$vo.cstname}</td>
            <td>{$vo.idno}</td>
            <td>{$vo.tel}</td>
            <td>{$vo.borrowamt}</td>
            <td>{$vo.duration}</td>
            <td>{$vo.rate}</td>
            <td>
                <eq name='vo.category' value='car'>车抵押&nbsp;<input type="button" value="&nabla;" onclick="demo08({$vo.id});" /></eq>
                <eq name='vo.category' value='fang'>房抵押&nbsp;<input type="button" value="&nabla;" onclick="demo08({$vo.id});"/></eq>
            </td>
            <td>
                {$vo.ostatus}
            </td>
            <td>
                   <a href='__URL__/modifyshow?id={$vo.id}'>修改</a>
              <!--   <if condition='$vo.status eq 1'> 
                <a href='__URL__/modifyshow?id={$vo.id}'>修改</a>
                <span class='confirm'>
                <a href='__URL__/del?id={$vo.id}'>删除</a>
                </span>
                </if> -->
            </td>
        </tr>
    </volist>
    
    </table>
     <!-- 我的个人信息结束 -->
     <div class='page'>
          {$page}
    </div>
</body>
